{
    "scriptId":  { "name": "org.jolokia.extra.addon.scripting.test.VersionTest",
                   "version": "1.0.0" },
    "meta": {
        "description": "check version call through BackendManager",
        "author": "MM"
    },
    "vars": {},        
    "collectionItems": [
               {
